<odoo>
<data>
    <!-- vlan tree view -->
    <record id="vlan_view_tree" model="ir.ui.view">
        <field name="name">vlan.view.tree</field>
        <field name="model">vlan</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="vlan_action_window">
        <field name="name">List VLAN</field>
        <field name="res_model">vlan</field>
        <field name="view_mode">tree,form</field>
    </record>
</data>
</odoo>